{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
        "esnext",
        "esnext.asynciterable",
        "dom"
    ],
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
        "~/*": [
            "./*"
        ],
        "@/*": [
            "./*"
        ]
    },
    "types": [
        "@types/node",
        "@nuxt/types"
    ],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
      "node_modules"
  ]
}
